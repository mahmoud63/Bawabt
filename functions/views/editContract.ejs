 

  <div class="content-container">
    <div class="container-fluid">
      <div class="jumbotron text-center">
        <h1>تعديل تعاقد</h1>
      </div>

      <form action="/editContract" method="POST" dir="rtl">
        <div class="form-group">
          <label> اسم التعاقد </label>
          <input
            type="text"
            name="name"
            class="form-control"
            value="<%= contracts[contract].Name %> "
          />
          <input type="hidden" name="contract" value="<%= contract %> " />
        </div>

        <div class="form-group">
          <input type="submit" value="تعديل" class="btn btn-primary btn-lg" />
        </div>
      </form>
    </div>
  </div>
</body>
